<div>
    <% PaymentMethod.human_attribute_types.each do |payment_method_type| %>
        <span>
        <%= t('.new_payment_setting_prefix') %>
        <%= link_to payment_method_type.downcase, root_path %>
        </span>
    <% end %>
</div>

<div>
  <% @payment_settings.each do |ps| %>
    <%= render partial: "payment_settings/#{ps.class}", locals: { payment_setting: ps } %>
  <% end %>
</div>
